<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>UserIdCheck</servlet-name>
        <servlet-class>com.feng.servlet.UserIdCheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserAdd</servlet-name>
        <servlet-class>com.feng.servlet.UserAdd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserLogin</servlet-name>
        <servlet-class>com.feng.servlet.UserLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginOut</servlet-name>
        <servlet-class>com.feng.servlet.LoginOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductShow</servlet-name>
        <servlet-class>com.feng.servlet.ProductShow</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartAdd</servlet-name>
        <servlet-class>com.feng.servlet.CartAdd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowCart</servlet-name>
        <servlet-class>com.feng.servlet.ShowCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartShpDel</servlet-name>
        <servlet-class>com.feng.servlet.CartShpDel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartShopNumsAdd</servlet-name>
        <servlet-class>com.feng.servlet.CartShopNumsAdd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartOrder</servlet-name>
        <servlet-class>com.feng.servlet.CartOrder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartPay</servlet-name>
        <servlet-class>com.feng.servlet.CartPay</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserIdCheck</servlet-name>
        <url-pattern>/userIdCheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserAdd</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserLogin</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginOut</servlet-name>
        <url-pattern>/loginout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductShow</servlet-name>
        <url-pattern>/productshow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartAdd</servlet-name>
        <url-pattern>/cartadd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowCart</servlet-name>
        <url-pattern>/showcart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartShpDel</servlet-name>
        <url-pattern>/cartshopdel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartShopNumsAdd</servlet-name>
        <url-pattern>/cartshopnumsadd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartOrder</servlet-name>
        <url-pattern>/cartorder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartPay</servlet-name>
        <url-pattern>/cartpay</url-pattern>
    </servlet-mapping>

</web-app>